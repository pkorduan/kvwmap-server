osm2po_IMAGE="pkorduan/osm2po-server"
osm2po_VERSION="5.1.0"

osm2po_env_vars="-e \"OSM2PO_VERSION=${osm2po_VERSION}\""
osm2po_volumes="-v ${USER_DIR}/etc/osm2po/config/osm2po.config:/usr/local/osm2po-${osm2po_VERSION}/osm2po.config \
                -v /var/docker/www/logs/osm2po:/usr/local/osm2po-${osm2po_VERSION}/logs"
