#!/bin/bash
GDAL_IMAGE=pkorduan/gdal-sshd
GDAL_IMAGE_VERSION=2.3.0
TERM=linux
COLUMNS=80
LINES=120
NETWORK_ALIAS=gdal
NETWORK=KVWMAP_PROD

# USER_DIR is defined in dcm

gdal_ports="-P"

gdal_env_vars="-e \"TERM=linux\" \
               -e PATH=$PATH:/usr/local/gdal/bin \
               -e PGPASSFILE=/root/.pgpass \
               -e \"TERM=${TERM}\" \
               -e \"COLUMNS=${COLUMNS}\" \
               -e \"LINES=${LINES}\""

gdal_volumes="-v $WWW_ROOT:/var/www \
              -v $USER_DIR/etc/postgresql/.pgpass:/root/.pgpass"
