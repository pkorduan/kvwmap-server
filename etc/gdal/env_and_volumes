#!/bin/bash
GDAL_IMAGE=pkorduan/gdal-sshd
GDAL_IMAGE_VERSION=2.3.0

# USER_DIR is defined in dcm
if [ -z $PGSQL_PASSFILE ] ; then
  read -p "Enter Name of PostgreSql pgpass password file: " PGSQL_PASSFILE
fi
gdal_env_vars="-e \"TERM=xterm\" \
               -e PGPASSFILE=/root/$PGSQL_PASSFILE"
gdal_volumes="--volumes-from wwwdata \
              -v $USER_DIR/etc/postgresql/.pgpass:/root/$PGSQL_PASSFILE"